    "express-session": "^1.17.3",
    "minimist": "^1.2.8",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0",
    "supertest": "^6.3.3"
  }
