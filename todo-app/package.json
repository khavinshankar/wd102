{
  "devDependencies": {
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0"
  },
  "lint-staged": {
